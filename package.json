{
  "name": "opencode-withvibes",
  "version": "0.1.0",
  "description": "OpenCode plugin for With Vibes providing persistent memory and context management via Zep Cloud",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "keywords": [
    "opencode",
    "plugin",
    "zep",
    "memory",
    "ai",
    "context",
    "withvibes"
  ],
  "author": "spikel1283",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/withvibes/opencode-withvibes.git"
  },
  "homepage": "https://github.com/withvibes/opencode-withvibes#readme",
  "bugs": {
    "url": "https://github.com/withvibes/opencode-withvibes/issues"
  },
  "bin": {
    "opencode-withvibes": "./bin/setup.js"
  },
  "dependencies": {
    "@getzep/zep-cloud": "^3.8.0",
    "chalk": "^5.3.0",
    "gray-matter": "^4.0.3",
    "ora": "^8.0.1",
    "p-queue": "^8.0.1",
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "@opencode-ai/plugin": "*"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.2",
    "@types/node": "^20.0.0",
    "bun-types": "^1.3.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "typescript": "^5.0.0"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "echo \"No tests yet\"",
    "prepare": "husky",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write ."
  },
  "lint-staged": {
    "*.{ts,js,json}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "files": [
    "bin",
    "dist",
    "skills",
    "scripts",
    "prompts",
    "opencode.json.example",
    "README.md",
    "LICENSE",
    "AGENTS.md"
